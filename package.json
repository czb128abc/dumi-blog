{
  "private": true,
  "name": "dumi-blog",
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.7",
    "dumi": "^1.1.25",
    "lint-staged": "^11.1.2",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.2.7",
    "umi-plugin-tailwindcss": "^3.1.5",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "antd": "^4.16.11",
    "babel-plugin-import": "^1.13.3"
  }
}
